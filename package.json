    "@coreui/coreui": "^5",
    "@coreui/icons": "^3.0.1",
    "@coreui/icons-react": "^2.3.0",
    "@coreui/react": "^5",
    "@eslint/js": "^9.29.0",
    "@popperjs/core": "^2.11.8",
    "@types/basic-auth": "^1.1.8",
    "@types/lodash": "^4.17.18",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.0.3",
    "@types/node-ssdp": "^4.0.4",
    "@types/primus": "^7.3.9",
    "@types/react": "^19.1.8",
